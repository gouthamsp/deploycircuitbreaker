{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "nginx",
      "Image": {
        "Name": "524932215262.dkr.ecr.ap-south-1.amazonaws.com/nginx:latest",
        "Update": true
      },
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["circuitbreaker"],
      "memory": 100
    },
    {
      "name": "circuitbreaker",
      "Image": {
        "Name": "524932215262.dkr.ecr.ap-south-1.amazonaws.com/circuitbreaker:latest",
        "Update": true
      },
      "hostname": "gateway",
      "essential": true,
      "memory": 300
    },
    {
      "name": "service1",
      "Image": {
        "Name": "524932215262.dkr.ecr.ap-south-1.amazonaws.com/service1:latest",
        "Update": true
      },
      "hostname": "service1",
      "essential": false,
      "memory": 300
    }
  ]
}