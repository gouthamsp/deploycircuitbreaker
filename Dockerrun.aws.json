{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "524932215262.dkr.ecr.ap-south-1.amazonaws.com/nginx:latest",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "circuitbreaker"
      ],
      "memory": 100
    },
    {
      "name": "circuitbreaker",
      "image": "524932215262.dkr.ecr.ap-south-1.amazonaws.com/circuitbreaker:latest",
      "hostname": "circuitbreaker",
      "essential": true,
      "links": [
        "service1"
      ],
      "memory": 300
    },
    {
      "name": "service1",
      "image": "524932215262.dkr.ecr.ap-south-1.amazonaws.com/service1:latest",
      "hostname": "service1",
      "essential": false,
      "memory": 300
    }
  ]
}